#!/Users/Stomp/.rvm/rubies/ruby-2.0.0-p195/bin/ruby
require File.join(File.dirname(__FILE__),'../lib/pso')

sphere_proc = Proc.new do |position|
  sphere_return = 0
  position.each do |position_i|
    sphere_return += position_i ** 2
  end
  next sphere_return
end

@dimensions = 20

Particle.min_position = -20
Particle.max_position = 20

Particle.max_velocity = 5
Particle.min_velocity = -5

@pso = PSO.new(20,@dimensions,sphere_proc)
2000.times do
  @pso.explore!
end
puts @pso.g_best.value
